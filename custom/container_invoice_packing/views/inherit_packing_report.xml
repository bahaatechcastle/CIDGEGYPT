<odoo>
    <data>
        <template id="report_packing_box" inherit_id="stock.report_picking">
            <xpath expr='//table' position="replace">
                <table class="table table-sm">
                    <thead>
                        <tr>
                            <th>
                                <strong>Number</strong>
                            </th>
                            <th>
                                <strong>Product</strong>
                            </th>
                            <th>
                                <strong>Code</strong>
                            </th>
                            <th>
                                <strong>Shape</strong>
                            </th>
                            <th>
                                <strong>surface</strong>
                            </th>
                            <th>
                                <strong>Lenght</strong>
                            </th>
                            <th>
                                <strong>Width</strong>
                            </th>
                            <th>
                                <strong>Thickness</strong>
                            </th>
                            <th>
                                <strong>No of Tiles</strong>
                            </th>
                            <th>
                                <strong>Qty</strong>
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        <!-- In case you come across duplicated lines, ask NIM or LAP -->
                        <t t-foreach="o.box_num" t-as="ml">
                            <tr>
                                <td>
                                    <span t-field="ml.box_num_id.name"/>
                                </td>
                                <td>
                                    <span t-field="ml.product_id.display_name"/>
                                </td>
                                <td>
                                    <span t-field="ml.product_id.item_code"/>
                                </td>
                                <td>
                                    <span t-esc="ml.product_id.shape"/>
                                </td>
                                <td>
                                    <span t-esc="ml.product_id.surface"/>
                                </td>
                                <td>
                                    <span t-esc="ml.product_id.lenght"/>
                                </td>
                                <td>
                                    <span t-esc="ml.product_id.width"/>
                                </td>
                                <td>
                                    <span t-esc="ml.product_id.thickness"/>
                                </td>
                                <td>
                                    <span t-esc="ml.product_id.no_of_tiles"/>
                                </td>
                                <td>
                                    <span t-esc="ml.product_id.product_uom_qty"/>
                                </td>
                            </tr>
                        </t>
                    </tbody>
                </table>
            </xpath>
        </template>
    </data>
</odoo>