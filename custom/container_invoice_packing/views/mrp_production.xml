<odoo>
    <data>
        <record id="view_notebook_shipping_mo_form" model="ir.ui.view">
            <field name="model">mrp.production</field>
            <field name="name">smrp.production.shipping.data</field>
            <field name="inherit_id" ref="mrp.mrp_production_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='product_id']"
                       position="after">
                    <field name="item_code"/>
                    <field name="lenght"/>
                    <field name="width"/>
                    <field name="thickness"/>
                    <field name="no_of_tiles"/>
                    <field name="shape"/>
                    <field name="surface"/>
                    <field name="order_id"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>